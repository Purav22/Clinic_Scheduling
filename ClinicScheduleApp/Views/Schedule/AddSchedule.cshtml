@model Schedule

@{
    ViewData["Title"] = "Add Schedule";
}

<form asp-controller="Schedule" asp-action="AddNewSchedule" method="post" enctype="application/x-www-form-urlencoded">
    <div class="text-danger" asp-validation-summary="All"></div>

    <partial name="_ScheduleFormPartial" />

    <button type="submit" class="btn btn-primary">Add</button>
    <a asp-controller="Schedule" asp-action="GetAllSchedules" class="btn btn-primary">Cancel</a>
</form>
